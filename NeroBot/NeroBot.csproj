<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<TargetFramework>net6.0-windows10.0.17763.0</TargetFramework>
		<UseWindowsForms>true</UseWindowsForms>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<Configurations>Debug;Release;Local;Hosted</Configurations>
		<Platforms>AnyCPU;x64</Platforms>
	</PropertyGroup>

	<ItemGroup>
	  <PackageReference Include="Discord.Net" Version="3.2.0" />
	  <PackageReference Include="Google.Apis.YouTube.v3" Version="1.55.0.2449" />
	  <PackageReference Include="Microsoft.Toolkit.Uwp.Notifications" Version="7.1.2" />
	  <PackageReference Include="TwitchLib" Version="3.3.0" />
	</ItemGroup>

	<ItemGroup>
		<Compile Update="BotCred.Designer.cs">
			<DesignTime>True</DesignTime>
			<AutoGen>True</AutoGen>
			<DependentUpon>BotCred.resx</DependentUpon>
		</Compile>
	</ItemGroup>

	<ItemGroup>
		<EmbeddedResource Update="BotCred.resx">
			<Generator>PublicResXFileCodeGenerator</Generator>
			<LastGenOutput>BotCred.Designer.cs</LastGenOutput>
		</EmbeddedResource>
	</ItemGroup>

</Project>
